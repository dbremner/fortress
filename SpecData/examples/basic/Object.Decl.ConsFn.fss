component Object.Decl.ConsFn

export Executable

trait List[\T\]
  append(List[\T\]):List[\T\]
end

object Empty[\T\]
  extends List[\T\]
  cons(x) = Cons(x,self)
  append(xs:List[\T\]):List[\T\] = xs
end

object Cons[\T\](first:T, rest:List[\T\])
  extends List[\T\]
  cons(x) = Cons(x,self)
  append(xs:List[\T\]):List[\T\] = Cons(first,rest.append(xs))
end
(** EXAMPLE **)
Cons[\T\](first:T) = Cons(first,Empty)
(** END EXAMPLE **)

run() = ()

end
