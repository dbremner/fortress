component Expr.Try.a

export Executable

read(x) = x
write(x,y) = x+y

f() = do
  file = 0
  newFile = file
(** EXAMPLE **)
try
  inp = read(file)
  write(inp, newFile)
forbid IOFailure
end
(** END EXAMPLE **)
end

run(args:String...) = println f()

end
