component AngelinaTest
export Executable

trait T
    x():ZZ32
end

object O(v:ZZ32)
    foo(): ZZ32 = 3
    (* After we move checking of method calls to be after
       type inference, remove the explicit return type T. *)
    bar():T = object extends T
                x():ZZ32 = 4
            end
    quack(): ZZ32 = self.v
end

run():() = do
    o:O = O(3);
    println o.v
    println o.bar().x()
end

end
