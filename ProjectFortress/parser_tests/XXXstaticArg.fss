component XXXstaticArg

export Executable

  t1[\Elt extends Number, nat N, [\Elt s\]
     \](): () = do

    a = array[\Elt\](N)
    a.fill(1)

    b = array[\Elt\](N)
    b.fill(2)

    c = array[\Elt\](N)
    c.fill(0)

    for i <- 0#N do
        a[i] := b[i] + s c[i]
    end

  end

  run(args:String...) :() = do

    t1[\RR64,200,3.0\]()

    t1[\ZZ32,200,3\]()

    t1[\ZZ64,200,3\]()

  end

end
