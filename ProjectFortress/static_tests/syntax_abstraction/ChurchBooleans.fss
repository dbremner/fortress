component ChurchBooleans

  export ChurchBooleans

  trait Boolean comprises {true, false}
    apply[\T\](x:T, y:T):T
  end

  object true extends Boolean
    apply[\T\](x:T, y:T):T = x
    toString():String = "true"
  end

  object false extends Boolean
    apply[\T\](x:T, y:T):T = y
    toString():String = "false"
  end

end
