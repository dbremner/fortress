 component MethodOnIntersection
  export Executable  
  trait A
    foo():()
  end
  trait B
    bar():()
  end
  
  run(args: String ...):() = ()
  
  quux(obj: A):() = typecase obj of
    B => do
      obj.foo()
      obj.bar()
    end 
    else => ()
  end
end