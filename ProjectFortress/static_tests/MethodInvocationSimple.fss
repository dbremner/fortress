component MethodInvocationSimple
  export Executable

  (* Check that some method call does in fact exist.
     No complicated overloading or inference. *)

  trait Foo
    doFoo[\A\](a:ZZ32,b:String):String
    doFoo[\A\](a:ZZ64,b:String):String

    doFoo[\A\](g:Generator[\A\],b:String) : ZZ32
  end

  methodCaller(a_1:ZZ32, b_1:String,f_1:Foo, g:Generator[\ZZ32\]) : () = do
    var s:String
    s := f_1.doFoo[\ZZ32\](a_1,b_1)

    (* Uh oh... this should work but doesn't
    var i:ZZ32
    i := f_1.dooFoo[\ZZ32\](g,b_1)
    *)
  end
  
  run(args:String...):() = ()

end