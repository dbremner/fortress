component While
  export Executable

  foo( m : Maybe[\String\], b:Boolean ) : () = do
    var s_void : ()
    s_void := while s <- m do
      var my_str : String
      my_str := s
    end
    s_void := while b do () end
  end

  run(arg:String...) : () = ()

end
