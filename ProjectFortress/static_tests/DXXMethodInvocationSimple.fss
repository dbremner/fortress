component DXXMethodInvocationSimple
  export Executable

  (* Check that some method call does in fact exist.
     No complicated overloading or inference. *)

  trait Foo
    doFoo[\A\](a:ZZ32,b:String):String
  end

  methodCaller(a_1:ZZ32, b_1:String,f_1:Foo) : () = do
    var s:String
    s := f_1.doFoo[\ZZ32\](a_1)
  end
  
  run(args:String...):() = ()

end