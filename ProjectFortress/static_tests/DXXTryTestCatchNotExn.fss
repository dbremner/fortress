component DXXTryTestCatchNotExn
  export Executable

  object Something end
  object SomethingElse extends Exception end
  object FooExn extends Exception end

  run(args:String...) : () = do
    var z:String
    z := try 
      throw FooExn
    catch e 
      FooExn => "First string"
      Something => "Second string"
    end
    ()
  end
end