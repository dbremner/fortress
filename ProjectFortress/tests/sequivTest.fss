component sequivTest
export Executable

equiv(a:Any, b:Any, other: Any...): () =
    if NOT (a SEQV b AND b SEQV a) then
        res = BIG STRING ("" other)
        fail(a " not SEQV " b ": " res)
    end


nequiv(a:Any, b:Any, other: Any...): () =
    if a SEQV b OR b SEQV a then
        res = BIG STRING ("" other)
        fail(a " not SEQV " b ": " res)
    end

run(args:String...) = do
    a : ZZ32 = 45
    b : RR64 = 45.0
    c : RR64 = 5.0 + 40.0
    d : ZZ64 = 1934137491234745 - 1934137491234700
    e : IntLiteral = 19341374912347452347629872905790283423598124792143168
    f : RR64 = b + e
    equiv(a,a)
    equiv(b,b)
    equiv(c,c)
    equiv(d,d)
    equiv(e,e)
    equiv(f,f)
    equiv(a,b)
    equiv(a,c)
    equiv(a,d)
    nequiv(a,e)
    nequiv(a,f)
    equiv(b,c)
    equiv(b,d)
    nequiv(b,e)
    nequiv(b,f)
    equiv(c,d)
    nequiv(c,e)
    nequiv(c,f)
    nequiv(d,e)
    nequiv(d,f)
    equiv(e,f)
    equiv((a,b),(a,c))
    equiv((),())
    nequiv((a,a),(a,f))
    nequiv((),a)
    nequiv(a,())
    equiv(true,true)
    equiv(false,false)
    nequiv(true,false)
    nequiv(true,17)
    n = Nothing[\ZZ32\]()
    j = Just[\ZZ32\](18)
    equiv(n,n)
    equiv(j,j)
    nequiv(n,j)
    nequiv(n,Nothing[\ZZ32\]())
    nequiv(j,Just[\ZZ32\](18))
    nequiv(n,a)
    nequiv(n,true)
    nequiv(j,a)
    nequiv(j,false)
    equiv((a,j),(b,j))
    nequiv((a,Nothing[\ZZ32\]()),(a,n))
  end

end
