component ObjectToStringTest
export Executable

object Foo(a: ZZ32, b:String) extends Object
end

object Fue[\T\](a: T, b:String) extends Object
end

object Bar(a: ZZ32, b:String)
end

run(args:String...) : () = do
    r = object end
    println(r.toString())
    println(Foo(17,"Hello there").toString())
    println(Fue[\ZZ32\](17,"Hello there").toString())
    println(Bar(17,"Hello there").toString())
  end

end
