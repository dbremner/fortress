component errIN
import ArrayList.{...}
export Executable

digits : List[\String\] =
    <| "1" asif String, "2", "3", "4", "5", "6", "7", "8", "9", "0" |>

run(args:String...):() = do
    curr = "K"
    typecase digits of
        Generator[\String\] => println("OK")
        else => println("Huh?")
    end
    println("************************************************************")
    if curr IN digits then
        println("FAIL")
    else
        println("OK")
    end
  end

end
