component printTests
export Executable

(* We can't easilly test that the right stuff is output, but we can test that 
   there is an overlaoding for each formulation *)
    
  run(args:String...) = do
    print ()
    print "Hello"
    print 1
    print ((1,2))
    print ((1, 2, 3))
    print ((1, 2, 3, 4))
    print ((1, 2, 3, 4, 5))
    print ((1, 2, 3, 4, 5, 6))
        
    println ()
    println "Hello"
    println 1
    println ((1,2))
    println ((1, 2, 3))
    println ((1, 2, 3, 4))
    println ((1, 2, 3, 4, 5))
    println ((1, 2, 3, 4, 5, 6))
  end

end
