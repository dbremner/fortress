component LooseJuxtMethodApplication

export Executable

object A(x:ZZ32)
    meth(y:ZZ32):ZZ32 = x + y
end

object B
    meth(y:ZZ32):ZZ32 = 5 + y
end

run(args:String...) = do
    o = A 5
    assert( o.meth(13), 18)
    assert( B.meth(7), 12)
    assert( o.meth 17, 22)
    assert( B.meth 31, 36)
    assert( (A 5).meth 23, 28 )
  end

end
