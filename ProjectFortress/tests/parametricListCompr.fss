component parametricListCompr
import List.{...}
import Set.{...}
import Map.{...}
export Executable

trait A extends StandardTotalOrder[\A\]
    opr =(self, other:A): Boolean = true
    opr <(self, other:A): Boolean = false
end
object B extends A end

foo(xs: List[\A\]) = ()
bar(xs: Set[\A\]) = ()
baz(xs: Map[\A,A\]) = ()

run(args) = do
    foo( <|[\A\] x | x <- <|B|> |> )
    bar( {[\A\] x | x <- {B} } )
    baz( {[\A,A\] x |-> x | x <- <|B|> } )
  end

end
