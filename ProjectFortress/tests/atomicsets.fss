component atomicsets

import Set.{...}
export Executable

initialSet():Set[\ZZ32\] = do
   init:Set[\ZZ32\] := set[\ZZ32\]()
   for i<-1#100 do
      atomic do
         init := init.add(i)
      end
   end
   init
end

run():()=do
   init:Set[\ZZ32\] = initialSet()
   assert(i IN init,true,i," NOTIN ",init), i <- 1#100
end
end
