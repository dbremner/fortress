component testTest1
export Executable


fib(n:ZZ32): ZZ32 =
  if n <= 1 then n
  else fib(n-1) + fib(n-2)
  end

test testFib1() = do
   assert(fib(0) = 0)
   assert(fib(5) = 5)
end

test testFib2() = do
  assert(fib(20),6765,"fib(20) wrong")
  assert(fib(25),75025,"fib(25) wrong")
end

end
