component testTest2
export Executable

fact(x:ZZ32) = do
   if (x = 0) then 1 else x fact(x-1) end
end

test testFactorial1() = do
   assert(fact(0) = 1)
   assert(fact(5) = 120)
   print("testFactorial1 passed");
end

