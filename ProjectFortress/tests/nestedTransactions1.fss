component nestedTransactions1

export Executable

test1():() = do
    var count : ZZ32 = 0
    atomic do 
        for i <- 1 # 5 do
            atomic do
                for j <- 1 # 5 do
                    atomic do
                       count := count + 1
                    end
                end
           end
        end
   end
   assert(count,    25, "test1 failed")
end

run(args:String...):()=do
    println("Starting test")
    recordTime(6.0)
    test1() 
    printTime(6.0)
end
end
