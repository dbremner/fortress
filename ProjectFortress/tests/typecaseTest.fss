component typecaseTest
export Executable

run():()=do
   x = "One Two Three "
   typecase _ = x of
      String => print("pass " x)
      ZZ32 => print("FAIL ")
      else => print("FAIL ")
   end

   y = 3.1415
   typecase _ = y of
      String => print("FAIL ")
      ZZ32 => print("FAIL ")
      Number => print("pass " y)
   end

   (a, b, c) = (1, 3.1415, "Hurrah")
   typecase _ = (a, b, c) of
    (Number, Number, Number) => print("FAIL ")
    (Number, Number, String) => print("pass " a "," b "," c)
   end

   (* Check that typecase returns a value! *)
   z = 3.1415
   q = typecase _ = z of
         String => "FAIL "
         ZZ32 => "FAIL "
         else => "pass " z
       end
   print(q)

   (* From a clause, too. *)
   w = 5
   p = typecase w of
          String => "FAIL"
          ZZ32 => "pass" w
          else => "FAIL"
       end
   println(p)
end
end
