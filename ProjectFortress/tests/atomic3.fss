component atomic3

export Executable

run():()=do
   var count : ZZ32 = 0
   println("Starting test")     
   for i <- 1#300 do
      atomic do count+= 1; count+=1; count+=1 end
   end
   if count=/=900 then
     println("FAIL: count = " count)
   end
end
end
