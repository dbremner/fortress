component SetTest
import Set.{...}
export Executable

  assertStr(a: TreeSet[\ZZ32\], b:TreeSet[\ZZ32\], s:String) =
  if a.toString() =/= b.toString() then
    fail(a "=/=" b ": " s)
  end


  run(args:String...):() = do
    a: TreeSet[\ZZ32\] = set[\ZZ32\]()
    b: TreeSet[\ZZ32\] = set[\ZZ32\](1#10)

    assertStr(a[:], set[\ZZ32\](), "subscript operator[:] incorrect for empty set")
    assertStr(b[:], b, "subscript operator[:] incorrect for tree set")
    assertStr(b[5:], set[\ZZ32\](6#5), "subscript operator[x:] incorrect for tree set")
    assertStr(b[9:9], singleton[\ZZ32\](10), "subscript operator[y:y] incorrect for tree set")
  end

end
