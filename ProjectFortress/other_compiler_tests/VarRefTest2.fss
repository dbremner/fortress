component VarRefTest2
export Executable

object Bar(x : ZZ32) end

object Foo
  x: ZZ32 = 42
end

run(): () = do
  b = Bar(42)
  f = Foo
  if b.x = f.x then println("PASS") else println("FAIL") end
end

end
