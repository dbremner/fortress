export Executable
(* testing overlap and coverage of method signatures and returns *)

trait T
  ee(T):T
  el(T):T
  le(T):T
  ll(T):T
  s():String
end

trait U extends T
  ee(T):T
  el(T):U
  le(x:U):T = x
  ll(U):U
end

trait V extends T
end

trait W extends T
  ee(T):T
  el(T):W
  le(W):T
  ll(x:W):W = x
end

object ot(ss:String) extends T
  ee(x:T):T = x
  el(x:T):T = x
  le(x:T):T = x
  ll(x:T):T = x
  s():String = ss
end

trait X extends {U,V}
  ee(x:T):T = ot("Xee")
end

object O extends {X, W}
  ee(x:T):T = ot("Oee")
  
  el(x:T):O = O

  le(x:T):T = ot("OTle")
  le(x:W):T = ot("OWle")

  ll(x:T):T = ot("Oll")
  ll(x:U):U = x

  s():String = "O"
end

run() = do
  t1:T = O
  t2:T = ot("foobarbaz")
  println(t1.ee(t2).s()) (* O.ee(T) = ot("Oee") .s() == "Oee"*)
  println(t1.el(t2).s()) (* O.el(T) = O .s() == "O"*)
  println(t1.le(t2).s()) (* O.le(T) = ot("OTle") .s() == "OTle" *)
  println(t1.le(t1).s()) (* O.le(W) = ot("OWle") .s() == "OWle" *)
  println(t1.ll(t2).s()) (* O.ll(T) = ot("Oll") .s() == "Oll" *)
  println(t1.ll(t1).s()) (* O.ll(U) = t1 = O .s() == "O" *)
end

