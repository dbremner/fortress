component Compiled170
export Executable

run():() = do
  a:ZZ32[5] = [0 1 2 3 4 5]
  prod:ZZ32 := 1
  s = spawn 
        for i <- a do
          atomic prod TIMES= i
        end
  t = spawn 
        for i <- a.indices do
          atomic prod TIMES= a[i]
        end
  s.join
  t.join
  println prod
end

end
