component Compiled10.Chain
export Executable

(* cf tests/chain2.fss, which is identical. *)

t(x:Boolean) = println( if x then "PASS" else "FAIL" end )

run() = do
  t( 0<=0<1 )
  t( 0<=0<1=1<2<=2 )
  t( NOT (0>=0>1=1>2>=2) )
end

end
