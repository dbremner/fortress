(*) Test working subscripting assignments.

  export Executable

  object O(var s: String)
    opr [i:ZZ32]: String = s
    opr [i:ZZ32] := (x: String) = s := x
  end

  run() = do
    var x: String = "hello"
    ((O(x))[5], x) ||= (x, " world")
  end
