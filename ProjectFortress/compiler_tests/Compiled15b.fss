export Executable

(*  Generic dotted methods in non-generic types *)

(* variant: inherit the generic method from a trait. *)

trait T
  f[\S\]():ZZ32
  g[\S, U\]():ZZ32
end

object O extends T
  f[\S\]():ZZ32 = 0
  g[\S, U\]():ZZ32 = f[\S\]() + 5
end

runt(t:T) =do
  a = t.f[\ZZ32\]()        (*) 0
  b = t.g[\ZZ32,String\]() (*) 5
  c = t.f[\String\]()      (*) 0
  d = t.g[\String,ZZ32\]() (*) 5

  sum =  a + b + c + d
  println(
  if (sum = 10)
     then "pass" else "fail" end
  )
end

run() = runt(O)
