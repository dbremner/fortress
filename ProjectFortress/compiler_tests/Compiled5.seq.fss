component Compiled5.seq
export Executable

(* Sequential version of fib by dint of (and in aid of testing) local
   variable binding. *)

fib(n: ZZ32): ZZ32 =
    if n <= 2 then
        1
    else
        t1 = fib(n-1)
        t2 = fib(n-2)
        t1+t2
    end

run(): () = printlnZZ32(fib 20)

end
