component Compiled16.a
export Executable

run():() = do
  a:ZZ32[5] = [0 1 2 3 4 5]
  prod1:ZZ32 := 1
  prod2:ZZ32 := 1
  s = spawn
        for i <- a do
          prod1 TIMES= i
        end
  t = spawn
        for i <- a.indices do
          prod2 TIMES= a[i]
        end
  println s.value
  println t.value
end

end
