component Compiled10
export Executable

(* Dotted methods. Also, inheritance of methods (or not). *)

trait A 
  m(a:A):String = a.m(self)
end

object O extends A 
  m(a:A):String = "O"
end

object P extends A 
  m(a:A):String = "P"
end

anO():A = O
anA():A = P

run():() = do
  println(anO().m(anA()))
end

end
