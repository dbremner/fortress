export Executable

(* New type checking rules for if exprs and coercions. *)

object A end
object B extends D end
object C end
trait D
  coerce(a: A) = Da
  coerce(c: C) = Dc
end
object Da extends D end
object Dc extends D end

(* Winning type is on an elif clause. *)
run() = do
  x: D = Da
  y: D = if true then A
         elif true then x
         elif true then C
         else B end
  println("PASS")
end
