component Comparison3
import ComparisonLibrary.{...}
export Executable

(*) Test various operators on the type Comparison.

run(): () = do
  U = Unordered
  L = LessThan
  G = GreaterThan
  E = EqualTo
  if NOT(CONVERSE U = U) then fail("Wanted CONVERSE U = U")
  elif NOT(CONVERSE L = G) then fail("Wanted CONVERSE L = G")
  elif NOT(CONVERSE G = L) then fail("Wanted CONVERSE G = L")
  elif NOT(CONVERSE E = E) then fail("Wanted CONVERSE E = E")
  else println("PASS")
  end
end

end
